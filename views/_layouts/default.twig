        {# <section aria-label="Search component">
          <form class="usa-search usa-search--small" role="search">
            <label class="usa-sr-only" for="search-field">Search</label>

            <input class="usa-input" id="search-field" type="search" name="search"/>

            <button class="usa-button" type="submit">
              <img
                src="{{ config.baseUrl }}uswds/img/usa-icons-bg/search--white.svg"
                class="usa-search__submit-icon"
                alt="Search"
              />
            </button>
          </form>
        </section> #}
      </nav>
    </div>
  </header>

  <div class="usa-section">
    <div class="grid-container">
      <div class="grid-row grid-gap">
        <div class="usa-layout-docs__sidenav desktop:grid-col-3">
          <nav aria-label="Secondary navigation">
            <ul class="usa-sidenav">
              {% if 'component' in tags %}
              {% set sidenav = {
                type: 'component',
                items: collections.componentByAlpha
              } %}
              {% endif %}

              {% if 'design_token' in tags %}
              {% set sidenav = {
                type: 'design_token',
                items: collections.design_tokenByAlpha
              } %}
              {% endif %}

              {% for item in sidenav.items %}
              <li class="usa-sidenav__item is-current">
                {% set url = item.url|split('/', 2) %}
                <a href="{{ config.baseUrl }}{{ url[0] ~ url[1] }}" {% if item.url == page.url %}class="usa-current" aria-current="page"{% endif %}>{{ item.data.title }}</a>

                {% if item.url == page.url %}
                <ul class="usa-sidenav__sublist">
                  <li class="usa-sidenav__item"><a href="#details">Details</a></li>

                  <li class="usa-sidenav__item"><a href="#examples">Examples</a></li>

                  <li class="usa-sidenav__item"><a href="#source-and-usage">Source and usage</a></li>

                  <li class="usa-sidenav__item"><a href="#design-library">Design library</a></li>
                </ul>
                {% endif %}
              </li>
              {% endfor %}
            </ul>
          </nav>
        </div>

        <main class="usa-layout-docs__main desktop:grid-col-9 usa-layout-docs" id="main-content">
          <div class="usa-prose">
            <h1>{{ title }}</h1>

            <p class="usa-intro">{{ intro }}</p>
          </div>

          {{ content|raw }}
        </main>
      </div>
    </div>
  </div>

  <footer class="usa-footer">
    <div class="grid-container usa-footer__return-to-top">
      <a href="#">Return to top</a>
    </div>

    <div class="usa-footer__primary-section">
      <nav class="usa-footer__nav" aria-label="Footer navigation">
        <ul class="grid-row grid-gap">
          {% for item in collections.primary %}
          <li class="mobile-lg:grid-col-4 desktop:grid-col-auto usa-footer__primary-content">
            {% set url = item.url|split('/', 2) %}
            <a class="usa-footer__primary-link" href="{{ config.baseUrl }}{{ url[0] ~ url[1] }}">{{ item.data.title }}</a>
          </li>
          {% endfor %}

          <li class="mobile-lg:grid-col-4 desktop:grid-col-auto usa-footer__primary-content">
            <a href="{{ package.design.url }}" target="_blank" rel="noopener nofollow" class="usa-footer__primary-link usa-link--external"><span>Design Library</span></a>
          </li>

          <li class="mobile-lg:grid-col-4 desktop:grid-col-auto usa-footer__primary-content">
            <a href="{{ package.repository.url }}" target="_blank" rel="noopener nofollow" class="usa-footer__primary-link usa-link--external"><span>v{{ package.version }}</span></a>
          </li>
        </ul>
      </nav>
    </div>

    <div class="usa-footer__secondary-section">
      <div class="grid-container">
        <div class="grid-row grid-gap">
          <div class=" usa-footer__logo grid-row mobile-lg:grid-col-6 mobile-lg:grid-gap-2">
            <div class="mobile-lg:grid-col-auto">
              <img class="usa-footer__logo-img" src="{{ config.baseUrl }}uswds/img/logo-img.png" alt=""/>
            </div>

            <div class="mobile-lg:grid-col-auto">
              <p class="usa-footer__logo-heading">The USWDS Honeycrisp theme</p>
            </div>
          </div>
        </div>
      </div>
    </div>
  </footer>

  <div class="usa-identifier padding-0">
    <section class="usa-identifier__section usa-identifier__section--masthead" aria-label="Agency identifier">
      <div class="usa-identifier__container">
        <div class="usa-identifier__logos">
          <a href="javascript:void(0)" class="usa-identifier__logo"><img class="usa-identifier__logo-img" src="{{ config.baseUrl }}uswds/img/circle-gray-20.svg" alt="&lt;Parent agency&gt; logo" role="img" /></a>
        </div>

        <section class="usa-identifier__identity" aria-label="Agency description">
          <p class="usa-identifier__identity-domain">codeforamerica.org</p>

          <p class="usa-identifier__identity-disclaimer">An official website of <a href="https://codeforamerica.org">Code for America</a></p>
        </section>
      </div>
    </section>
  </div>

  <script src="{{ config.baseUrl }}uswds/js/uswds.js"></script>

  {% block scripts %}{% endblock %}
</body>
</html>